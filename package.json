{
  "name": "simple-bootstrap",
  "version": "1.0.0",
  "description": "Basic Wordpress theme using Boostrap. Built to be very clean.",
  "main": "Gruntfile.js",
  "dependencies": {
    "bootstrap": "^4.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "postcss-cli": "^6.1.3",
    "sass": "^1.22.9"
  },
  "scripts": {
    "build": "sass scss/style.scss style.css -I . && postcss style.css --use autoprefixer -r -m",
    "dist": "grunt dist",
    "dev": "grunt watcher",
    "test": "echo \"no test specified\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nicolas-van/wordpress-simple-boostrap.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nicolas-van/wordpress-simple-boostrap/issues"
  }
}
